{
    "_id": "biobb_mem",
    "desc": "Description for biobb_mem package",
    "github": "https://github.com/bioexcel/biobb_mem",
    "readthedocs": "https://biobb-mem.readthedocs.io/en/latest/",
    "conda": "",
    "docker": "",
    "singularity": "",
    "version": "5.0.0",
    "tools" : [
        {
            "block" : "CHAP", 
            "tool" : "chap", 
            "desc" : "Channel Annotation Package (CHAP) for analyzing pore geometry, hydrophobicity, and hydration state in protein channels and other macromolecular structures.",
            "exec" : "chap_run",
            "docs": "https://biobb-mem.readthedocs.io/en/latest/chap.html#module-chap.chap"
        },
        {
            "block" : "CpptrajDensity", 
            "tool" : "density",
            "desc" : "Calculates the density along an axis of a given cpptraj compatible trajectory.",
            "exec" : "density",
            "docs": "https://biobb-mem.readthedocs.io/en/latest/ambertools.html#module-ambertools.cpptraj_density"
        },
        {
            "block" : "AssignLeaflets", 
            "tool" : "assign_leaflets", 
            "desc" : "Assign lipids to leaflets in a bilayer.",
            "exec" : "assign_leaflets",
            "docs": "https://biobb-mem.readthedocs.io/en/latest/lipyphilicBB.html#lipyphilicBB-assign-leaflets-module"
        }
    ],
    "dep_pypi" : [
        "install_requires=['biobb_common==5.0.0', 'mdanalysis>=2.1.0']", 
        "python_requires='>=3.9'"
    ], 
    "dep_conda" : [
        "python >=3.9", 
        "biobb_common ==5.0.0",
        "ambertools >=22.0",
        "lipyphilic ==0.10.0",
        "chap"
    ],
    "keywords" : [
        "membrane", 
        "keywords"
    ]
}